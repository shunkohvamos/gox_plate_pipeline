{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extract clean CSV (260203-1)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/extract_clean_csv.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--raw",
        "data/raw/260203-1",
        "--row_map",
        "data/meta/260203-1.tsv",
        "--config",
        "meta/config.yml"
      ],
      "justMyCode": true
    },
    {
      "name": "Fit rates+REA (260203-1)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/fit_initial_rates.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--tidy", "data/processed/260203-1/extract/tidy.csv",
        "--config", "meta/config.yml",
        "--out_dir", "data/processed",
        "--plot_dir", "data/processed/plots",
        "--min_points", "6",
        "--max_points", "30",
        "--r2_min", "0.97",
        "--slope_min", "0.0",
        "--max_t_end", "600",
        "--min_span_s", "0",
        "--min_delta_y", "0"
      ],
      "justMyCode": true
    },
    {
      "name": "Extract clean CSV (260203-2)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/extract_clean_csv.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--raw",
        "data/raw/260203-2",
        "--row_map",
        "data/meta/260203-2.tsv",
        "--config",
        "meta/config.yml"
      ],
      "justMyCode": true
    },
    {
      "name": "Fit rates+REA (260203-2)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/fit_initial_rates.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [
        "--tidy", "data/processed/260203-2/extract/tidy.csv",
        "--config", "meta/config.yml",
        "--out_dir", "data/processed",
        "--plot_dir", "data/processed/plots",
        "--min_points", "6",
        "--max_points", "30",
        "--r2_min", "0.97",
        "--slope_min", "0.0",
        "--max_t_end", "600",
        "--min_span_s", "0",
        "--min_delta_y", "0"
      ],
      "justMyCode": true
    },
    {
      "name": "Generate launch.json from data",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/generate_launch_json.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [],
      "justMyCode": true
    },
    {
      "name": "Generate TSV template from raw",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/generate_row_map_template.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": [],
      "justMyCode": true
    },
    {
      "name": "Python デバッガー - 引数を含む現在のファイル（都度入力）",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "python": "${workspaceFolder}/.venv/bin/python",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "args": "${command:pickArgs}"
    }
  ]
}
